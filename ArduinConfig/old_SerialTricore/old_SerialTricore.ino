/* CPU1 Initialised Data */
StartOfInitialised_CPU1_Variables
uint32_t lastPulseus = 0;
uint32_t us = 0;
//Hadamard sequence 4096 points
const int hdmN = 4095;
int VADCn = 0;
uint32_t pulseWidth = 15;
int driftcode = 0; //0 Hadamard, 1-9 pulse width
EndOfInitialised_CPU1_Variables

void setup1() {
  pinMode(13, OUTPUT);
  pinMode(12, OUTPUT);
  pinMode(11, OUTPUT);
  analogReadResolution(12);
}


void loop1() {
  for (int i = 0; i < hdmN; i++) //80ns
  {
    
    if (driftcode == 9)
    {
      Fast_digitalWrite(13, 0); // 70ns  start with 0 for 20% of the pulse us
      delayMicroseconds(pulseWidth / 5);
      Fast_digitalWrite(13, hdm[i]);
    }
    //without chopped
    do {
      us = micros();
    } while (us - lastPulseus < pulseWidth);
    if (driftcode == 0)
      Fast_digitalWrite(13, hdm[i]); // 70ns
    if (driftcode > 0 && driftcode < 9) // 1-8
      Fast_digitalWrite(13, (i < (driftcode * 2))); // 70ns v. 1.2 added *2

                           //Fast_digitalWrite(11, hdm[(i + hdmN - 1000) % hdmN]); // 70ns  //simulate delayed pulse on pin 11
    delayMicroseconds(1);
    lastPulseus = us;
    VADC_result[i] += ReadAD0(); //2800 ns
  }
  VADCn++;
}



