void setup2() {
  SerialASC.begin(2000000);
  printData();
}

void loop2() {
  if (SerialASC.available() > 0)
  {
    char ch = SerialASC.read();//one char
    driftcode = ch - '0';
    ch = SerialASC.read();//second char
    pulseWidth = (ch - '0') * 5;
    printData();
  }
}

void printlnHex(int num) {
  char tmp[16];
  sprintf(tmp, "%06X", num & 0xFFFFFF);
  SerialASC.println(tmp);
}

void printData() {
  printlnHex(driftcode * 16 + pulseWidth / 5);

  SerialASC.println("______");
  //copy the acquired data to a buffer
  for (int i = 0; i < hdmN; i++)
  {
    VADC_buffer[i] = VADC_result[i];
    VADC_result[i] = 0;
  }
  VADCnbuffer = VADCn;
  VADCn = 0;

  //print the buffer to the serial port
  for (int i = 0; i < hdmN; i++)
  {
    printlnHex(VADC_buffer[i]);
  }
  SerialASC.println("......");
  printlnHex(VADCnbuffer);
}



